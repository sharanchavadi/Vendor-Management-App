<%= form_for(@vendor) do |f| %>
 

  <div class="field">
    <%= f.label :first_name %><br>
    <%= f.text_field :first_name %>
  </div>
  <div class="field">
    <%= f.label :last_name %><br>
    <%= f.text_field :last_name %>
  </div>
  <div class="field">
    <%= f.label :phone_number %><br>
    <%= f.text_field :phone_number %>
  </div>
  <div class="field">
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :business_name %><br>
    <%= f.text_field :business_name %>
  </div>
  <div class="field">
    <%= f.label :tin_number %><br>
    <%= f.text_field :tin_number %>
  </div>

  <div class="field">
    <%= f.label :business_type %><br>
    <%= f.collection_check_boxes :business_type_ids, BusinessType.all, :id, :name %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
   
  

<% end %>
